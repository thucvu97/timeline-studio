    "@tauri-apps/plugin-websocket": "~2.3.0",
    "@xstate/react": "^5.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "idb-keyval": "^6.2.2",
    "lucide-react": "^0.511.0",
    "next": "^15.1.8",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-audio-visualize": "github:eadwinCode/react-audio-visualize",
