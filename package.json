  "author": "Alexander Kireyev <ak.chatman.media@gmail.com>",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:windows": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint:fix:windows": "eslint --fix src/**/*.{ts,tsx}",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix",
    "format:imports": "eslint --fix \"src/**/*.{ts,tsx}\" --rule \"import/order: warn\"",
    "format:imports:windows": "eslint --fix src/**/*.{ts,tsx} --rule \"import/order: warn\"",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "lint:rust:fix": "cd src-tauri && cargo clippy --fix --allow-dirty -- -D warnings",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:rust:check": "cd src-tauri && cargo fmt --check",
    "check:rust": "npm run lint:rust && npm run format:rust:check",
    "fix:rust": "npm run format:rust && npm run lint:rust:fix",
    "check:all": "npm run lint && npm run lint:css && npm run format:imports && npm run check:rust && npm run test",
    "fix:all": "npm run lint:css:fix && npm run format:imports && npm run fix:rust",
    "tauri": "tauri",
    "test": "vitest run",
    "test:app": "vitest run src/features",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "npm run test:coverage && ./bin/deepsource report --analyzer test-coverage --key javascript --value-file ./coverage/lcov.info",
    "test:coverage:rust": "cd src-tauri && source \"$HOME/.cargo/env\" && cargo llvm-cov --lcov --output-path coverage.info",
    "test:coverage:rust:report": "npm run test:coverage:rust && ./bin/deepsource report --analyzer test-coverage --key rust --value-file ./src-tauri/coverage.info",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "playwright:install": "playwright install",
    "docs": "node scripts/generate-docs.js",
    "docs:watch": "nodemon --watch src --ext ts,tsx --ignore '**/*.test.ts' --ignore '**/*.test.tsx' --exec 'node scripts/generate-docs.js'",
    "promo:dev": "cd promo && bun run dev",
    "promo:build": "cd promo && bun run build",
    "promo:preview": "cd promo && bun run preview"
  },
  "dependencies": {
